# Copy this file to .env and fill in the secrets/URLs.

# Firecrawl API key (required for Research panel)
FIRECRAWL_API_KEY=

# Optional: Excalidraw collab server URL (for future live collaboration)
# Example: https://your-excalidraw-room.example.com
EXCALIDRAW_ROOM_URL=

# Supabase (planned next)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# AI SDK (provider via OpenAI-compatible API)
OPENAI_API_KEY=
# Optional: set this when using an OpenAI-compatible provider like OpenRouter/Together
OPENAI_BASE_URL=
# Default model (overridable in API call)
AI_MODEL=gpt-4o-mini

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000

